/**
 * Implements hook_context_default_contexts().
 */
function custom_mtech_context_default_contexts() {
  $files = file_scan_directory(drupal_get_path('module', 'custom_mtech'). '/contexts/context', '/inc$/');
  foreach ($files as $filename => $file) {
    require $filename;
    $contexts[$file->name] = $context;
  }
  
  return $contexts;
}
